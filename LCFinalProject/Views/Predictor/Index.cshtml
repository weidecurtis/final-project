@model LCFinalProject.ViewModels.PredictorViewModel


<h1> Catchers </h1>
<div class="row">
    <div class="table-responsive col-lg-2">
        <table class="table-bordered table-responsive">
            <thead>
                <tr>
                    <th colspan="2" style="text-align:center">Last 10 Games Average</th>
                </tr>
                <tr>
                    <th style="text-align:center">Name</th>
                    <th style="text-align:center">Score</th>
                </tr>
            </thead>
            @foreach (var catcher in Model.Catchers)
            {
                <tr>
                    <td style="text-align:center" width="75%">@catcher.Name</td>
                    <td style="text-align:center">@catcher.ProjectedScore</td>
                </tr>
            }
        </table>
    </div>
    <div class="table-responsive col-lg-2">
        <table class="table-bordered">
            <thead>
                <tr>
                    <th colspan="2" style="text-align:center">Away Stats</th>
                </tr>
                <tr>
                    <th style="text-align:center" width="75%">Name</th>
                    <th style="text-align:center">Score</th>
                </tr>
            </thead>
            @foreach (var catcher in Model.AwayCatchers)
            {
                <tr>
                    <td style="text-align:center" width="75%">@catcher.Name</td>
                    <td style="text-align:center">@catcher.TotalScore</td>
                </tr>

            }
        </table>
    </div>
    <div class="table-responsive col-lg-2">
        <table class="table-bordered">
            <thead>
                <tr>
                    <th colspan="2" style="text-align:center" width="75%">Home Stats</th>
                </tr>
                <tr>
                    <th style="text-align:center">Name</th>
                    <th style="text-align:center">Score</th>
                </tr>
            </thead>
            @foreach (var catcher in Model.HomeCatchers)
            {
                <tr>
                    <td style="text-align:center" width="75%">@catcher.Name</td>
                    <td style="text-align:center">@catcher.TotalScore</td>
                </tr>
            }
        </table>
    </div>
    <div class="table-responsive col-lg-2">
        <table class="table-bordered">
            <thead>
                <tr>
                    <th colspan="2" style="text-align:center">Versus Right Handers</th>
                </tr>
                <tr>
                    <th style="text-align:center">Name</th>
                    <th style="text-align:center">Score</th>
                </tr>
            </thead>
            @foreach (var catcher in Model.RhpCatchers)
            {
                <tr>
                    <td style="text-align:center" width="75%">@catcher.Name</td>
                    <td style="text-align:center">@catcher.TotalScore</td>
                </tr>
            }
        </table>
    </div>
    <div class="table-responsive col-lg-2">
        <table class="table-bordered">
            <thead>
                <tr>
                    <th colspan="2" style="text-align:center">Versus Left Handers</th>
                </tr>
                <tr>
                    <th style="text-align:center">Name</th>
                    <th style="text-align:center">Score</th>
                </tr>
            </thead>
            @foreach (var catcher in Model.LhpCatchers)
            {
                <tr>
                    <td style="text-align:center" width="70%">@catcher.Name</td>
                    <td style="text-align:center">@catcher.TotalScore</td>
                </tr>
            }
        </table>
    </div>
</div>


